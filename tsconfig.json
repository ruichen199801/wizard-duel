{
  "compilerOptions": {
    "jsx": "react-jsx",
    "strict": true,
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noEmit": true,
    "baseUrl": "./src",
    "paths": {
      "@ai": [
        "ai"
      ],
      "@components/*": [
        "components/*"
      ],
      "@core/*": [
        "core/*"
      ],
      "@hooks": [
        "hooks"
      ],
      "@utils": [
        "utils"
      ],
    }
  },
  "include": [
    "src/**/*"
  ],
}